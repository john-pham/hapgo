<!-- Page Heading -->
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">
            <i class="fa fa-dashboard"></i> <small>Transactions</small>
        </h1>
        <ol class="breadcrumb">
            <li class="active">
                <i class="fa fa-dashboard"></i>
                <p class="input-group">
                    <input type="text" class="form-control" id="dt_searchdate" />
                    <span class="input-group-btn">
                        <button type="button" class="btn btn-default"><i class="fa fa-calendar"></i></button>
                    </span>
                </p>
            </li>
        </ol>
    </div>
</div>



<!-- /.row -->
<div class="row">
    <div class="panel panel-default">
        <div class="panel-body">
            <div class="table-responsive">
                <table class="table table-bordered table-hover table-condensed" ng-init="app.init()">
                    <thead>
                        <tr>
                            <td><strong>#Time</strong></td>
                            <td><strong>#Account</strong></td>
                            <td><strong>Bookmaker</strong></td>
                            <td><strong>BM-Status</strong></td>
                            <td><strong>R-Time</strong></td>
                            <td><strong>Status</strong></td>
                            <td><strong>IP</strong></td>
                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="item in app.betlist">
                            <td>{{item.CreatedDateString}}</td>
                            <td><span ng-class="{'label label-default': item.Status==2, 'label label-primary': item.Status==3, 'label label-danger': item.Status==1,'label label-success': item.Status>3}">{{item.UserName}}</span></td>
                            <td>{{item.SboReference}}</td>
                            <td>{{item.SboStatus}}</td>
                            <td>{{item.ResponseTimeString}}</td>
                            <td>{{item.StatusString}}</td>
                            <td>{{item.IP}}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="panel-footer">
            @Html.Partial("_Pagination")
        </div>
    </div>
</div>